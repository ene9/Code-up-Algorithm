#include <stdio.h>

int main(void){
    
    int a, b, c;
    float sum;
    scanf("%d %d %d", &a, &b, &c);
    
    sum = a+b+c;
    printf("%d\n", a+b+c);
    printf("%.1f\n", sum/3);
    
    return 0;
}



----------------------------------------


    int a, b, c;  // 1 2 3 
    float sum;
    scanf("%d %d %d", &a, &b, &c);
    
    sum = a+b+c;
    printf("%d\n", sum);  // 처음에 이렇게 썼었는데, 잘못된 풀이였다. 
    printf("%.1f\n", sum/3);
    
    sum = 6.000 이렇게 될 줄 알았는데 아직 이해가 안된다....
    
    
