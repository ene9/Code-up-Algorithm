#include <stdio.h>

int main(void){
    
    int n;
    scanf("%d", &n);
    while(n!=0){
        printf("%d\n", n-1);
        n--;
    }
    return 0;
}

------------------------------------

#include <stdio.h>

int main(void){
    
    int n;
    scanf("%d", &n);
    for(int i=0; i<n; i++){
        printf("%d\n", n-(i+1));
    }
    return 0;
}
