#include <stdio.h>

int main(void){
    
    int a = 0;
    int x;
    scanf("%d", &x);
    
    while(a <= x){
        printf("%d\n", a);
        a++;
    }
    
    return 0;
}


--------------------------------------------------


#include <stdio.h>

int main(void){
    
    int x;
    scanf("%d", &x);
    
    for(int i=0; i<=100; i++){
        if(i>x)
            break;
        printf("%d\n", i);
    }
    
    return 0;
}
